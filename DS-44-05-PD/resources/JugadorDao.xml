<?xml version="1.0" encoding="UTF-8"?>

<querys>
	<query name="findAll">
		<script>SELECT * FROM JUGADOR;</script>
	</query>
	<query name="findOne">
		<script>SELECT * FROM JUGADOR WHERE id = [id];</script>
	</query>
	<query name="add" var="jugador">
		<script>INSERT INTO JUGADOR VALUES ([jugador.id],[jugador.name]);</script>
	</query>
	<query name="delete" var="jugador">
		<script>DELETE FROM JUGADOR WHERE ID = [jugador.id];</script>
	</query>
</querys>